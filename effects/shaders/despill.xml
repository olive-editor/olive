<?xml version="1.0" encoding="UTF-8"?>
<effect name="Despill" category="Keying">
    <row name="Channel">
		<field type="combo" default="1" id="channel">
			<option>Red</option>
			<option>Green</option>
			<option>Blue</option>
		</field>
	</row>
    
    <row name="Factor">
		<field type="double" min="0" max="100" default="100" id="factorIn"/>
	</row>
    
    <row name="Balance">
		<field type="double" min="-100" max="100" default="0" id="balanceIn"/>
	</row>

        <shader vert="common.vert" frag="despill.frag"/>
</effect>
