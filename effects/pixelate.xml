<?xml version="1.0" encoding="UTF-8"?>
<effect name="Pixelate" category="Stylize">
	<row name="Horizontal Pixels">
		<field type="double" min="0" default="16" id="pixels_x"/>
	</row>
	<row name="Vertical Pixels">
		<field type="double" min="0" default="9" id="pixels_y"/>
	</row>
	<row name="Bypass">
		<field type="bool" default="0" id="bypass"/>
	</row>
	<shader vert="common.vert" frag="pixelate.frag"/>
</effect>