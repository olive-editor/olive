      <memberdef kind="slot" id="class_action_search_1ad9ce9f5a257569b695f30c0cc5304b7b" prot="private" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void ActionSearch::search_update</definition>
        <argsstring>(const QString &amp;s, const QString &amp;p=nullptr, QMenu *parent=nullptr)</argsstring>
        <name>search_update</name>
        <param>
          <type>const QString &amp;</type>
          <declname>s</declname>
        </param>
        <param>
          <type>const QString &amp;</type>
          <declname>p</declname>
